<Window x:Class="WPF_Koleje_Studenckie_project_Jakub_Bak.AdminPanelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Administration Panel" Height="400" Width="600">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock FontSize="24" FontWeight="Bold" Text="Administration Panel" Margin="0,0,0,20"/>
            <Button Content="Train Management" Margin="5" Padding="10" Click="TrainManagement_Click"/>
            <Button Content="Cargo Management" Margin="5" Padding="10" Click="CargoManagement_Click"/>
            <Button Content="Personnel Management" Margin="5" Padding="10" Click="PersonnelManagement_Click"/>
            <Button Content="Schedule Management" Margin="5" Padding="10" Click="ScheduleManagement_Click"/>
        </StackPanel>
    </Grid>
</Window>
